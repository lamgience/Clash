# 2. DNS Ê∑±Â∫¶‰ºòÂåñ (DNS Configuration)
# ------------------------------------------------------------------------------
dns:
  enable: true
  listen: :53
  ipv6: true
  # [Ê®°Âºè] Fake-IP (Â¢ûÂº∫Ê®°Âºè)
  # ‰ΩúÁî®ÔºöËøîÂõûËôöÊãü IP ÂÆûÁé∞ÁßíÂºÄÁΩëÈ°µÔºåÂπ∂Âú®Êú¨Âú∞ÁºìÂ≠òÊò†Â∞ÑÂÖ≥Á≥ª
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  fake-ip-filter-mode: blacklist
  # [ÈªëÂêçÂçï] Âº∫Âà∂Ëµ∞ÁúüÂÆû IP Ëß£ÊûêÁöÑÂüüÂêç
  # ‰ΩúÁî®ÔºöËß£ÂÜ≥Â±ÄÂüüÁΩëËÆæÂ§áÂèëÁé∞„ÄÅËøûÈÄöÊÄßÊµãËØï„ÄÅSTUN„ÄÅÂæÆËΩØÊúçÂä°Á≠âÂÖºÂÆπÊÄßÈóÆÈ¢ò
  fake-ip-filter:
    - "*.lan"
    - "*.local"
    - "*.arpa"
    - "time.*.com"
    - "ntp.*.com"
    - "+.market.xiaomi.com"
    - "localhost.ptlogin2.qq.com"
    - "*.msftncsi.com"
    - "www.msftconnecttest.com"
  
  # [ÈªòËÆ§ DNS] Áî®‰∫éËß£Êûê DoH ÂüüÂêçÁöÑ Bootstrap DNS
  default-nameserver:
    - 223.5.5.5
    - 119.29.29.29
  
  # [ÂõΩÂ§ñ DNS] Áî®‰∫éËß£ÊûêÂõΩÂ§ñÂüüÂêç (DoH)
  nameserver:
    - https://doh.pub/dns-query
    - https://dns.alidns.com/dns-query
    - 8.8.8.8
  
  # [Fallback] ÂõûÈÄÄ DNS ÈÖçÁΩÆ
  fallback: []
  fallback-filter:
    geoip: true
    geoip-code: CN
    ipcidr: [240.0.0.0/4]
  
  # [ÂàÜÊµÅÁ≠ñÁï•] Split-DNS (Ê†∏ÂøÉ‰ºòÂåñ)
  # ÈÄªËæëÔºöGeosite ÂåπÈÖçÂà∞ÁöÑ CN ÂüüÂêç -> ÂõΩÂÜÖ DNSÔºõÂÖ∂‰ªñ -> ÂõΩÂ§ñ DNS
  nameserver-policy:
    "geosite:cn,private":
      - https://doh.pub/dns-query
      - https://dns.alidns.com/dns-query
    "geosite:geolocation-!cn":
      - https://dns.google/dns-query
      - https://1.1.1.1/dns-query

